<h1>Transactions</h1>

<ul>
  <% @transactions.each do |transaction| %>
    <li>
      <a href="/transactions/<%= transaction.id %>">
        <%= "Amount: " %>
        <%= transaction.amount %>
        <%= "/ Date: " %>
        <%= transaction.date %>
        <%= "/ Description: " %>
        <%= transaction.description %>
        <%= "/ Bank ID: " %>
        <%= transaction.bank_id %>
      </a>
    </li>
  <% end %>
</ul>
